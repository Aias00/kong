# these are set by kong core, some used in setup_env.sh and kong-build-tools
# kong-distributions also uses them, and translates them into the naming
# convention over there

KONG_PACKAGE_NAME=kong-enterprise-edition
PACKAGE_CONFLICTS=kong-community-edition
PACKAGE_PROVIDES=kong-enterprise-edition
PACKAGE_REPLACES=kong-community-edition
DOCKER_RELEASE_REPOSITORY=kong/kong-gateway
KONG_LICENSE="ASL 2.0"

EDITION=enterprise

KONG_GMP_VERSION=6.2.1
RESTY_VERSION=1.21.4.1
RESTY_BORINGSSL_VERSION=b3ed071ecc4efb77afd0a025ea1078da19578bfd
RESTY_LUAROCKS_VERSION=3.9.1
RESTY_PCRE_VERSION=8.45
RESTY_LMDB_VERSION=1.0.0
RESTY_WEBSOCKET_VERSION=0.3.0
LIBYAML_VERSION=0.2.5
KONG_GO_PLUGINSERVER_VERSION=v0.6.1
RESTY_OPENSSL_VERSION=1.1.1q
RESTY_EVENTS_VERSION=0.1.3
ATC_ROUTER_VERSION=1.0.0
KONG_BUILD_TOOLS_VERSION=4.33.22
KONG_NGINX_MODULE_BRANCH=0.2.1

## XXX EE

# here go enterprise build specific ones which might or might not have the
# same names as core for building the final package.

KONG_DISTRIBUTIONS_VERSION=master

KONG_DEP_NETTLE_VERSION=3.7.3
KONG_DEP_LIBYAML_VERSION=0.2.5
KONG_DEP_LICENSING_VERSION=0.3.0
KONG_DEP_LUA_RESTY_OPENSSL_AUX_MODULE_VERSION=0.2.0
KONG_DEP_PASSWDQC_VERSION=1.3.1

# opentracing stuff
KONG_DEP_OPENTRACING_CPP_VERSION=v1.6.0
KONG_DEP_DATADOG_VERSION=v1.3.0
KONG_DEP_NGINX_OPENTRACING_VERSION=v0.14.0
KONG_DEP_JAEGER_VERSION=v0.7.0

LYAML_VERSION=6.2.7

KONG_PGMOON_VERSION=2.3.0

KONG_DEP_LIBJQ_VERSION=1.5

# see post-install-admin-portal.sh for additional defaults/variables

# KONG_ADMIN_DIRECTORY=gui
# KONG_ADMIN_VERSION=nightly

KONG_PORTAL_DIRECTORY=portal
KONG_PORTAL_VERSION=v1.3.0-1

## EE

# whether to enable bytecompilation of kong lua files or not
# leave blank or set to empty string to avoid bytecompilation
ENABLE_LJBC=true

# whether a patch to kong.conf.default disabling anonymous reports is applied
# leave blank or set to empty string to avoid applying patch
ENABLE_ANONYMOUS_REPORTS=

# whether a patch to LICENSE for government contract purposes is applied
# leave blank or set to empty string to avoid applying patch
ENABLE_GDIT_PATCH=

# whether to install the external C kong-licensing library
# this should always be set to true when building distribution artifacts
ENABLE_KONG_LICENSING=true

LUASEC_VERSION=1.2.0
